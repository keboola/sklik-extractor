{
    "name": "keboola/sklik-extractor",
    "type": "symfony-bundle",
    "description": "Keboola Sklik extractor",
    "keywords": ["keboola", "sklik", "extractor"],
    "authors": [
        {
            "name": "Jakub Matejka",
            "email": "jakub@keboola.com"
        }
    ],
    "repositories": [
        {
            "type": "composer",
            "url": "https://packages.zendframework.com/"
        }
    ],
    "require": {
        "keboola/syrup": "~2.0",
        "zendframework/zend-xmlrpc": "2.3.*"
    },
    "require-dev": {
        "phpunit/phpunit": "~4.4",
        "symfony/var-dumper": "~2.6",
        "squizlabs/php_codesniffer": "2.*",
        "codeclimate/php-test-reporter": "dev-master"
    },
    "scripts": {
        "post-install-cmd": [
            "Keboola\\Syrup\\DeploymentHandler\\ScriptHandler::getParameters",
            "Keboola\\Syrup\\DeploymentHandler\\ScriptHandler::getSharedParameters",
            "Sensio\\Bundle\\DistributionBundle\\Composer\\ScriptHandler::buildBootstrap"
        ],
        "post-update-cmd": [
            "Keboola\\Syrup\\DeploymentHandler\\ScriptHandler::getParameters",
            "Keboola\\Syrup\\DeploymentHandler\\ScriptHandler::getSharedParameters",
            "Sensio\\Bundle\\DistributionBundle\\Composer\\ScriptHandler::buildBootstrap",
            "Sensio\\Bundle\\DistributionBundle\\Composer\\ScriptHandler::clearCache"
        ]
    },
    "minimum-stability": "stable",
    "autoload": {
        "psr-0": {
            "Keboola\\SklikExtractor": ""
        }
    },
    "target-dir": "Keboola/SklikExtractor",
    "extra": {
        "symfony-app-dir": "vendor/keboola/syrup/app",
        "symfony-web-dir": "vendor/keboola/syrup/web",
        "syrup-app-name": "ex-sklik"
    }
}
