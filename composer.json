{
    "name": "keboola/sklik-extractor-bundle",
    "type": "symfony-bundle",
    "description": "Keboola Sklik extractor Bundle",
    "keywords": ["keboola", "sklik", "extractor"],
    "authors": [
        {
            "name": "Jakub Matejka",
            "email": "jakub@keboola.com"
        }
    ],
    "repositories": [
        {
            "type": "vcs",
            "url": "git@bitbucket.org:keboola/extractor-bundle.git"
        },
        {
            "type": "composer",
            "url": "https://packages.zendframework.com/"
        }
    ],
    "require": {
        "keboola/extractor-bundle": "~1.0.0-beta",
        "zendframework/zend-xmlrpc": "2.3.*"
    },
    "require-dev": {
    },
    "scripts": {
        "post-install-cmd": [
            "Syrup\\CoreBundle\\DeploymentHandler\\ScriptHandler::getParameters",
            "Syrup\\CoreBundle\\DeploymentHandler\\ScriptHandler::getSharedParameters",
            "Sensio\\Bundle\\DistributionBundle\\Composer\\ScriptHandler::buildBootstrap"
        ],
        "post-update-cmd": [
            "Syrup\\CoreBundle\\DeploymentHandler\\ScriptHandler::getParameters",
            "Syrup\\CoreBundle\\DeploymentHandler\\ScriptHandler::getSharedParameters",
            "Sensio\\Bundle\\DistributionBundle\\Composer\\ScriptHandler::buildBootstrap",
            "Sensio\\Bundle\\DistributionBundle\\Composer\\ScriptHandler::clearCache"
        ]
    },
    "minimum-stability": "stable",
    "autoload": {
        "psr-0": {
            "Keboola\\SklikExtractorBundle": ""
        }
    },
    "target-dir": "Keboola/SklikExtractorBundle",
    "extra": {
        "symfony-app-dir": "vendor/keboola/syrup/app",
        "symfony-web-dir": "vendor/keboola/syrup/web",
        "syrup-app-name": "ex-sklik"
    }
}
